{
  "name": "lidash",
  "version": "0.1.0",
  "description": "Array and object immutable helpers inspired by lodash",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maikelrobier/lidash.git"
  },
  "keywords": [
    "array",
    "object",
    "immutable",
    "utilities",
    "lodash"
  ],
  "author": "Maikel Ruiz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/maikelrobier/lidash/issues"
  },
  "homepage": "https://github.com/maikelrobier/lidash#readme",
  "husky": {
